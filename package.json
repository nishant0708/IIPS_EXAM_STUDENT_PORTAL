  "description": "- Primary Background color : #1A1A1A\r - secondary Background Color : #282828\r - text color : white (primary)\r - secondary text color : #Eff2f699\r - secondary text color : #Eff1f6bf (Darker for headings)",
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "com.example.student_portal",
    "productName": "StudentPortal",
    "directories": {
      "output": "dist"
    },
    "files": [
  "build/**/*",
  "index.js",
  "package.json",
  "resources/**/*"
],
"asarUnpack": [
  "resources/**/*"
],
    "win": {
      "target": "nsis",
      "icon": "icon.ico",
      "signAndEditExecutable": false,
      "requestedExecutionLevel": "requireAdministrator"
    },
    "mac": {
      "target": "dmg",
      "icon": "icon.icns"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
